<%- include("partials/_headers") %>

  <body>

    <%- include("partials/_navigations") %>

      <!-- Page Header -->
      <header class="masthead" style="background-image: url('img/post-bg.jpg')">
        <div class="overlay"></div>
        <div class="container">
          <div class="row">
            <div class="col-lg-8 col-md-10 mx-auto">
              <div class="post-heading">
                <h1>
                  <%= post.title %>
                </h1>
                <h2 class="subheading">
                  <%= post.detail %>
                </h2>
                <span class="meta">
                  <%= post.createdAt %>
                </span>
              </div>
            </div>
          </div>
        </div>
      </header>

      <%- include("partials/_footers") %>

  </body>

  </html>